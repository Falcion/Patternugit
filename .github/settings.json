        {
          "id": 2,
          "type": "User"
        }
      ],
      "deployment_branch_policy": {
        "protected_branches": true
      }
    },
    {
      "name": "development",
      "deployment_branch_policy": {
        "custom_branches": [
          "main",
          {
            "name": "dev/*",
            "type": "branch"
          },
          {
            "name": "release/*",
            "type": "branch"
          },
          {
            "name": "v*",
